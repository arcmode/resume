doctype 5
html
	head
		title= title
		link(rel='stylesheet', href='/stylesheets/style.css')
		link(rel='stylesheet', href='/stylesheets/gradient.css')
	body
		#wrapper
			.navbar.navbar-fixed-top.navbar-inverse
				.navbar-inner
					.container
						a.btn.btn-navbar(data-toggle="collapse", data-target=".nav-collapse")
						a.brand(href="#")= title
						.nav-collapse
							ul.nav
								block navResume
									li
										a(href="/") Resume
								li.divider
								block navContact
									li
										a(href="/contact") Contact
			#main.container
				#content
					block content
		include includes/footer
		include includes/loading
	block scripts
		script(src="/javascripts/jquery-1.9.0.min.js")
		script(src="/javascripts/jquery-ui-1.10.0.custom.min.js")
		script(src="/javascripts/loader.js")